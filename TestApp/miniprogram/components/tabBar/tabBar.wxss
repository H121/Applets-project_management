/* components/tabBar/tabBar.wxss */
.boxs_2 {
  width: 650rpx;
  /* height: 100rpx; */
  display: flex;
  position: fixed;
  bottom: 0px;
  padding: 10rpx 50rpx;
  color: #000;
  justify-content: space-between;
  background-color: #fff;
  font-size: 28rpx;
  box-shadow: 0rpx 2rpx 50rpx 6rpx #8bbce4;
}

.boxs_2 view {
  text-align: center;
}
/* 选中 Bar */
.boxs_2 .view_selt{
	color: #5074ff
}

.boxs_2 image {
  width: 50rpx;
  height: 50rpx;
}
.boxs_2 .big_img .tianbao {
  position: absolute;
  top: -25px;
  width: 50px;
  height: 50px;
  margin-left: 50%;
  left: -25px;
}


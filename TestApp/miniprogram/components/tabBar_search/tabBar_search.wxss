/* components/tabBar_search/tabBar_search.wxss */
.searchContainer {
  padding: 0 20rpx;
}
.header {
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  padding: 10rpx 0;
}
.searchInput {
  position: relative;
  flex: 1;
  background: rgba(237, 237, 237, 0.3);
  border-radius: 30rpx;
  z-index: 9;
}
.cancel {
  width: 100rpx;
  text-align: center;
  z-index: 10;
}
.searchInput input {
  margin-left: 50rpx;
  height: 60rpx
}